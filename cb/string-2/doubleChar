public String doubleChar(String str) {
  if (str.length() == 0){
    return "";
  } else {
    return str.substring(0,1) + str.substring(0,1) + doubleChar(str.substring(1));
  }
}
=======
public String doubleChar(String str) {
  String c = "";
  for (int i = 0; i < str.length(); i++){
    c = c + str.substring(i,i+1) + str.substring(i,i+1);
  }
  return c;
}
